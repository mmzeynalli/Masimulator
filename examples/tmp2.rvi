    c.addi x1, 0              # loop counter
    addi x2, x0, 1
    addi x3, x0, 9              # loop limit
    addi x4, x0, 1              # fibonacci no. 1
    addi x5, x0, 1              # fibonacci no. 2
LOOP:
    addi x1, x1, 1              # counter++
    nop
    nop
    beq x3, x1, DONE            # if counter == loop limit then done!
    add x4, x4, x5
    add x5, x5, x4
    beq x0, x0, LOOP            # jump
DONE:
    sw x4, 0(x0)
    sw x5, 4(x0)
